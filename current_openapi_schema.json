        "is_published",
        "last_updated",
        "ll_bbox_polygon",
        "metadata_author",
        "name",
        "pk",
        "poc",
        "polymorphic_ctype_id",
        "processed",
        "raw_abstract",
        "raw_constraints_other",
        "raw_data_quality_statement",
        "raw_purpose",
        "raw_supplemental_information",
        "sourcetype",
        "state",
        "store",
        "thumbnail_url",
        "title",
        "uuid",
        "workspace"
    ]
}